{
    "name": "ravin-regmi/laradoo",
    "description": "Odoo ERP API for Laravel",
    "keywords": [
        "odoo",
        "laravel",
        "erp",
        "package"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ravin Regmi",
            "email": "ravin.regmi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-xmlrpc": "*",
        "illuminate/support": "^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Edujugon\\Laradoo\\": "src/"
        },
        "classmap": [
            "src/ripcord"
        ],
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "minimum-stability": "dev"
}
